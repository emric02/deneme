import java.util.Scanner;

public class Uygulama {

    public  static  void main(String[] args)
    {

        Scanner klavye=new Scanner(System.in);
        System.out.println("Matematik dersi Not sonucu: ");
        System.out.println("Vize  sonucu: ");
        double vize=klavye.nextDouble();
        System.out.println("Final sonucu: ");
        double vize1=klavye.nextDouble();
        double not;
        not=(vize*0.4+vize1*0.6);
        if(not>50)
        {
            System.out.println("MATEMATİK DERSİNDEN   " +not+ " GEÇTİNİZ...");
        }
        else
            System.out.println("MATEMATİK DERSİNDEN  " +not+ " ile  KALDINIZ.MAALESEF SENEYE BİR DAHA ALACAKSINIZ..");
    }

}
